{
  "name": "@bloomreach/example-saas",
  "description": "Example React SPA for brX SaaS",
  "version": "14.3.0",
  "private": true,
  "dependencies": {
    "@bloomreach/react-sdk": "^14.3.0",
    "@bloomreach/spa-sdk": "^14.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2",
    "@fortawesome/free-solid-svg-icons": "^5.14",
    "@fortawesome/react-fontawesome": "^0.1",
    "axios": "^0.19",
    "bootstrap": "^4.5",
    "react": "^16.13",
    "react-bootstrap": "^1.3",
    "react-dom": "^16.13",
    "react-google-maps": "^9.4",
    "react-player": "^2.6",
    "react-router-dom": "^5.2"
  },
  "devDependencies": {
    "@bloomreach/eslint-config": "^0.1",
    "@types/react": "^16.9",
    "@types/react-dom": "^16.9",
    "@types/react-router-dom": "^5.1",
    "eslint": "^7.9",
    "react-scripts": "3.4.1",
    "typescript": "^3.8"
  },
  "scripts": {
    "build": "react-scripts build",
    "lint": "eslint src/",
    "start": "react-scripts start"
  },
  "eslintConfig": {
    "extends": "@bloomreach/eslint-config",
    "overrides": [
      {
        "files": [
          "*.tsx"
        ],
        "rules": {
          "react/require-default-props": "off"
        }
      }
    ],
    "rules": {
      "react/jsx-props-no-spreading": "off",
      "react/no-danger": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
